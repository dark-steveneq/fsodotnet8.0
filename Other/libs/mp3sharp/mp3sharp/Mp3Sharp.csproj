<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>TargetFramework</TargetFramework>
    <ProjectType>Local</ProjectType>
    <AssemblyKeyContainerName />
    <AssemblyOriginatorKeyFile />
    <DefaultClientScript>JScript</DefaultClientScript>
    <DefaultHTMLPageLayout>Grid</DefaultHTMLPageLayout>
    <DefaultTargetSchema>IE50</DefaultTargetSchema>
    <DelaySign>false</DelaySign>
    <OutputType>Library</OutputType>
    <RootNamespace />
    <RunPostBuildEvent>OnBuildSuccess</RunPostBuildEvent>
    <StartupObject />
    <TargetFramework>net8.0</TargetFramework>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <AssemblyTitle>Mp3Sharp</AssemblyTitle>
    <Description>Mp3 Decoder for the .NET Framework</Description>
    <Company>Robert Burke (rob@mle.ie)</Company>
    <Copyright>100% Freeware</Copyright>
    <AssemblyVersion>1.4.*</AssemblyVersion>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <BaseAddress>285212672</BaseAddress>
    <ConfigurationOverrideFile />
    <DefineConstants />
    <FileAlignment>4096</FileAlignment>
    <NoStdLib>false</NoStdLib>
    <NoWarn />
    <RegisterForComInterop>false</RegisterForComInterop>
    <RemoveIntegerChecks>false</RemoveIntegerChecks>
    <WarningLevel>1</WarningLevel>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <BaseAddress>285212672</BaseAddress>
    <ConfigurationOverrideFile />
    <DefineConstants />
    <FileAlignment>4096</FileAlignment>
    <NoStdLib>false</NoStdLib>
    <NoWarn />
    <Optimize>false</Optimize>
    <RegisterForComInterop>false</RegisterForComInterop>
    <RemoveIntegerChecks>false</RemoveIntegerChecks>
    <WarningLevel>1</WarningLevel>
    <DebugType>none</DebugType>
    <UseVSHostingProcess>true</UseVSHostingProcess>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)' == 'ServerRelease|AnyCPU'">
    <OutputPath>bin\ServerRelease\</OutputPath>
    <BaseAddress>285212672</BaseAddress>
    <WarningLevel>1</WarningLevel>
    <FileAlignment>4096</FileAlignment>
    <CodeAnalysisRuleSet>MinimumRecommendedRules.ruleset</CodeAnalysisRuleSet>
  </PropertyGroup>
  <ItemGroup>
    <Reference Update="System">
      <Name>System</Name>
    </Reference>
    <Reference Update="System.Data">
      <Name>System.Data</Name>
    </Reference>
    <Reference Include="System.Design">
      <Name>System.Design</Name>
    </Reference>
    <Reference Update="System.Xml">
      <Name>System.XML</Name>
    </Reference>
  </ItemGroup>
  <ItemGroup>
    <Content Include="decoder\readme.txt" />
    <Content Include="readme.txt" />
    <None Update="Support\_ConversionReport.htm">
      <SubType>Preview</SubType>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.Management" Version="8.0.0" />
  </ItemGroup>
  <ItemGroup>
    <Compile Remove="converter\Converter.cs" />
    <Compile Remove="converter\jlc.cs" />
    <Compile Remove="decoder\BitstreamOLD.cs" />
    <Compile Remove="Mp3StreamOLD.cs" />
    <Compile Remove="player\AudioDevice.cs" />
    <Compile Remove="player\AudioDeviceBase.cs" />
    <Compile Remove="player\AudioDeviceFactory.cs" />
    <Compile Remove="player\FactoryRegistry.cs" />
    <Compile Remove="player\JavaSoundAudioDevice.cs" />
    <Compile Remove="player\JavaSoundAudioDeviceFactory.cs" />
    <Compile Remove="player\jlp.cs" />
    <Compile Remove="player\NullAudioDevice.cs" />
    <Compile Remove="player\Player.cs" />
    <Compile Remove="player\PlayerApplet.cs" />
  </ItemGroup>
  <PropertyGroup />
</Project>